{
    "name": "__MSG_appName__",
    "description": "__MSG_appDescription__",
    "version": "12",
    "manifest_version": 2,
    "default_locale": "en",
    "permissions": [
        {
            "socket": [
                "tcp-listen:*:*",
                "tcp-connect:*:*",
                "udp-bind:*:*",
                "udp-send-to:*:*"
            ]
        },
        "app.window",
        "audioCapture",
        "contextMenus",
        "desktopCapture",
        "fullscreen"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/main.js",
                "scripts/chromereload.js"
            ]
        }
    }
}